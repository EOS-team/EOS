/dts-v1/;
/plugin/;

/{
	compatible = "brcm,bcm2711";

	fragment@0 {
		target = <&uart2>;
		__overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&uart2_pins>;
			status = "okay";
		};
	};

	fragment@1 {
		target = <&uart2_pins>;
		__dormant__ {
			brcm,pins = <0 1 2 3>;
			brcm,pull = <0 2 2 0>;
		};
	};

	__overrides__ {
		ctsrts = <0>,"=1";
	};
};
